<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<!-- edu.zju.cims201.GOF.hibernate.pojo.Author root -->
	<class name="edu.zju.cims201.GOF.hibernate.pojo.MaterialEmission" table="MaterialEmission">
		<id name="id" type="int" column="id">
			<generator class="increment"/>
		</id>
		<property name="materialId" column="materialId" type="int" />
		<property name="componentId" column="componentId" type="int" />
		<property name="emission" column="emission" type="java.lang.Double" />
		<property name="sum" column="sum" type="java.lang.Double" />
		<property name="userId" column="userId" type="int" />
	</class>
</hibernate-mapping>
