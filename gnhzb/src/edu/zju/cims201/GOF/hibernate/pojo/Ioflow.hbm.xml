<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<!-- edu.zju.cims201.GOF.hibernate.pojo.Author root -->
	<class name="edu.zju.cims201.GOF.hibernate.pojo.Ioflow" table="Ioflow">
		<id name="id" type="int" column="id">
			<generator class="increment"/>
		</id>
	    <property name="name" column="name" type="string"/>
	    <property name="unitcost" column="unitcost" type="int"/>
	    <property name="amount" column="amount" type="int"/>
	    <property name="unit" column="unit" type="string"/>
	    <property name="type" column="type" type="string"/>
	</class>

</hibernate-mapping>
